<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


  <VBox alignment="TOP_CENTER" fillWidth="false" spacing="15" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LogIn.LogInController">
   <Label text="Client App" textAlignment="CENTER" textFill="#0098eb">
      <font>
         <Font size="35.0" />
      </font>
   </Label>
   <Label text="Please enter your username" VBox.vgrow="ALWAYS" />
      <TextField fx:id="usernameField" alignment="CENTER" promptText="Enter username" VBox.vgrow="ALWAYS" />
      <Button fx:id="LogInButton" onAction="#LogInButtonPressed" text="Log In" VBox.vgrow="ALWAYS" />
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
  </VBox>
